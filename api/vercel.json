{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/api/auth",
      "dest": "/routes/auth.js"
    },
    {
      "src": "/api/user",
      "dest": "/routes/user.js"
    },
    {
      "src": "/api/post",
      "dest": "/routes/post.js"
    },
    {
      "src": "/api/catagories",
      "dest": "/routes/catagories.js"
    }
  ]
}
